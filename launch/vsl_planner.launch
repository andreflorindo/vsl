<?xml version="1.0" ?>
<launch>
  <param name="ikfast_base_frame" type="string" value="kr210_base_link" />
  <param name="ikfast_tool_frame" type="string" value="penholder_tool_tip" />      <!--kr210_qr_tool0-->

  <node name="pose_builder" pkg="vsl_core" type="pose_builder" output="screen">
    <param name="world_frame" value="kl2000_base_link"/>  <!--table_table_top-->
  </node>
  
  <node name="course_display" pkg="vsl_core" type="course_display" output="screen">
    <param name="world_frame" value="kl2000_base_link"/>  <!--table_table_top-->
    <param name="visualization/min_point_distance" value="0.02"/> 
  </node>

  <node name="cartesian_publisher" pkg="vsl_core" type="cartesian_publisher" output="screen">
    <param name="tip_link" value="penholder_tool_tip"/>    <!--kr210_qr_tool0-->
    <param name="base_link" value="kr210_base_link"/>
  </node>

  <node name="vsl_planner_descartes" pkg="vsl_core" type="vsl_planner_descartes" output="screen">
    <param name="group_name" value="manipulator"/>
    <param name="tip_link" value="penholder_tool_tip"/>    <!--kr210_qr_tool0-->
    <param name="base_link" value="kr210_base_link"/>
    <param name="world_frame" value="kl2000_base_link"/>  <!--table_table_top-->
    <!-- <param name="trajectory/time_delay" value="0.1"/> -->
    <rosparam param="trajectory/seed_pose">[1.57, -1.57, 1.04 , 0.0, 1.57, 0.0]</rosparam>
    <param name="visualization/min_point_distance" value="0.02"/> 
  </node>
</launch>
